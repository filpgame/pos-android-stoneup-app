<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/receiptLayout">

    <ImageView
            android:layout_width="332dp"
            android:layout_height="184dp" app:srcCompat="@drawable/ic_stoneup_top"
            android:id="@+id/imageView7"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="16dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp" android:layout_marginTop="4dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.6"/>
    <ImageView
            android:layout_width="304dp"
            android:layout_height="216dp" tools:srcCompat="@tools:sample/avatars[5]"
            android:id="@+id/imageView8"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="141dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="142dp" android:layout_marginTop="4dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView7" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView9"/>
    <ImageView
            android:layout_width="346dp"
            android:layout_height="154dp" app:srcCompat="@drawable/ic_stoneup_bottom"
            android:id="@+id/imageView9"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="4dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="11dp" app:layout_constraintHorizontal_bias="0.421"/>
</androidx.constraintlayout.widget.ConstraintLayout>